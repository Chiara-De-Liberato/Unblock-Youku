{
  "name": "Unblock Youku Lite",
  "version": "1.5",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
  "description": "For browsing Youku/Tudou/Xiami/Ku6/Xunlei online videos/audios from overseas, without proxy!",
  "icons": { "16": "icons/lite-16.png",
             "48": "icons/lite-48.png",
            "128": "icons/lite-128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
